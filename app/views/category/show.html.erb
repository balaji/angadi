<% content_for(:title, "Browse #{category.name} - Angadi") %>
<div class="container">
    <%= render partial: 'category/crumbs', locals: { category: category } %>
    <div class="row">
        <div class="col-xs-3"></div>
        <div class="col-xs-9 text-center">
            <h2><%= category.name %></h2>
        </div>
    </div>
    <%= render partial: 'products', locals: {
            categories: [category],
            products: products
    } %>
</div>
